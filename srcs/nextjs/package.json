{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:docker": "bun run fetch-contract-addresses && next dev --turbopack",
    "dev": "bun run export-to-frontend && next dev --turbopack --port 5173",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "fetch-contracts": "node scripts/fetch-contract-addresses.js",
    "export-to-frontend": "node scripts/export-to-frontend.js",
    "typechain-healthcare-registration": "typechain --target ethers-v6 --out-dir ./utils/types/HealthcareRegistration '../foundry/out/HealthcareRegistration.sol/HealthcareRegistration.json'",
    "typechain-healthcare-prover": "typechain --target ethers-v6 --out-dir ./utils/types/HealthcareRegistrationProver '../foundry/out/HealthcareRegistrationProver.sol/HealthcareRegistrationProver.json'",
    "typechain-zkMed": "bun run typechain-healthcare-registration && bun run typechain-healthcare-prover",
    "typechain": "bun run typechain-zkMed",
    "build:standalone": "NEXT_OUTPUT=standalone next build",
    "container:build": "docker build -t zkmed-frontend .",
    "container:run": "docker run -p 3000:3000 zkmed-frontend"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@reown/appkit": "^1.7.10",
    "@reown/appkit-adapter-wagmi": "^1.7.10",
    "@tanstack/react-query": "^5.80.6",
    "@vlayer/react": "^1.2.3",
    "@vlayer/sdk": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "thirdweb": "^5.102.6",
    "tw-animate-css": "^1.3.4",
    "usehooks-ts": "^3.1.1",
    "uuid": "^10.0.0",
    "viem": "^2.31.0",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "typechain": "^8.3.2",
    "@typechain/ethers-v6": "^0.5.1"
  }
}
